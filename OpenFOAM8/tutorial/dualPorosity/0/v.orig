FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      u;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    top
    {
        type            fixedValue;
        value           uniform 5e5;
    }

    bottom
    {
        type            fixedValue;
        value           uniform 0;
    }

    left
    {
        type    zeroGradient;
        // type            noFlux;
    }

    right
    {
        type    zeroGradient;
        // type            noFlux;
    }

    frontAndBack
    {
        type            empty;
    }
}


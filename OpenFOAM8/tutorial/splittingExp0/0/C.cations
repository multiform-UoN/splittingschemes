FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      C.cations;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 0 0 0 0 ];

internalField   uniform 1;

boundaryField
{
    cathode
    {
        type noFlux;
        /*
        type            RobinPhi;
        RobinK 0;
        RobinF 0;
        RobinD 0.1;
        phi "phiNP";
        value           uniform 1;
        */
    }
    anode
    {
        type noFlux;
        /*
        type            RobinPhi;
        RobinK 0;
        RobinF 0;
        RobinD 0.1;
        phi "phiNP";
        value           uniform 1;
        fluxICoeffs     uniform 0;
        fluxBCoeffs     uniform 0;
        snGrad          uniform 0;
        */
    }

    "top|bottom"
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}


FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      mvu;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 0 0 0 0 0];

internalField   uniform -1;

boundaryField
{
    left
    {
        type            fixedValue;
        value           uniform -1;
    }
    right
    {
        type            fixedValue;
        value           uniform -1;
    }
    bottom
    {
        type            fixedValue;
        value           uniform -1;
    }
    top
    {
        type            fixedValue;
        value           uniform -1;
    }
    frontAndBack
    {
        type            empty;
    }
}



FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      mvv;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 0 0 0 0 0];

val 1.0;

//internalField   uniform $val;

 mvInit  #codeStream
 {
        codeInclude
        #{
            #include "fvCFD.H"
        #};

        codeOptions
        #{
            -I$(LIB_SRC)/finiteVolume/lnInclude \
            -I$(LIB_SRC)/meshTools/lnInclude
        #};

        codeLibs
        #{
            -lmeshTools \
            -lfiniteVolume
        #};

        code
        #{
            scalar pi(constant::mathematical::pi);
            const IOdictionary& d = static_cast<const IOdictionary&>(dict);
            const fvMesh& mesh = refCast<const fvMesh>(d.db());
            scalarField fld
             (
               Foam::pow
               (
               10,
               1.0
               *
               Foam::cos(scalar(2.)*pi*mesh.C().component(0)->primitiveField())
               *
               Foam::cos(scalar(2.)*pi*mesh.C().component(1)->primitiveField())
               )
               ///
               //scalar(2)
             );
             writeEntry(os,"",fld);
         #};


         #inputMode merge
 };

internalField $mvInit;

boundaryField
{
    "left|right|top|bottom"
    {
        // type            calculated;
        type            fixedValue;
        value           uniform     $val;
    }

    frontAndBack
    {
        type            empty;
    }
}


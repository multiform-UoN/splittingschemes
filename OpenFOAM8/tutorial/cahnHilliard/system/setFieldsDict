FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue     U     0
);

regions
(
    boxToCell
    {
        box (3.2 0 0.1) (3.6 1 0.4);
        fieldValues
        (
            volScalarFieldValue     U       1
        );
    }
);


